        <a href="<%= path_to_url video.asset.url %>"><%= video.name %></a>
      </p>
      <!--[if gt IE 6]><!-->
    </object><!--<![endif]-->
    <!--[if gt IE 6]></object><![endif]-->
  </object>
</video>